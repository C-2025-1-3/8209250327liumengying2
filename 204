#include<iostream>
using namespace std;
int main() {
	float a, c;
	char b;
	cin >> a >> b >> c;
	switch (b) {
	case '+':cout << a + c; break;
	case '-':cout << a - c; break;
	case '*':cout << a * c; break;
	case '/':if (c != 0) {
		cout << a / c;
	}
		  else {
			cout << "error";

		}break;
	default:cout << "error"; break;
}
	return 0;
